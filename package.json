{
	"name": "klasa",
	"version": "0.5.0-dev",
	"description": "Klasa: Croatian for 'class', is a class based remix on Komada.",
	"main": "src/index.js",
	"types": "typings/index.d.ts",
	"author": "@skyra",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "eslint src typings --ext js,ts --fix",
		"format": "prettier --write src/**/*.js typings/**/*.ts",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz"
	},
	"dependencies": {
		"fs-nextra": "^0.5.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@skyra/eslint-config": "^5.3.3",
		"@types/node": "^14.14.6",
		"@types/ws": "^7.2.9",
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"cz-conventional-changelog": "^3.3.0",
		"discord.js": "^12.4.1",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"lint-staged": "^10.5.1",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"rimraf": "^3.0.2",
		"standard-version": "^9.0.0",
		"typescript": "^4.0.5"
	},
	"peerDependencies": {
		"discord.js": "12.x"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/skyra-project/klasa.git"
	},
	"files": [
		"src",
		"typings"
	],
	"keywords": [
		"klasa",
		"discordjs",
		"discord.js",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"skyra"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/klasa/issues"
	},
	"homepage": "https://github.com/skyra-project/klasa/",
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"acorn": "^7.3.1",
		"minimist": "^1.2.2",
		"kind-of": "^6.0.3",
		"dot-prop": "^5.2.0"
	},
	"engines": {
		"node": ">=12"
	}
}
